class Solution {
public:
    int minSwaps(string s) {
        int n = s.size() ; 
        int cnt =  0; 
        
        stack<char>st ; 

        for(auto x : s ) {
            if(x == '['){
                st.push(x) ; 
            }
            else {
                if(!st.empty() )
                {
                    st.pop(); 
                }
                else {
                    cnt++ ; 
                }
            }
        }

        return (cnt + 1)/2 ; 

    }
};
